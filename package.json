{
  "name": "duosphere-api",
  "version": "1.0.0",
  "description": "Mobile-first real-time study collaboration app",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev:worker": "wrangler dev",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "wrangler deploy",
    "db:apply": "wrangler d1 execute duosphere --file=server/schema.sql",
    "lint": "eslint src --ext ts,tsx",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@cloudflare/workers-types": "^4.20250204.0",
    "@types/bun": "latest",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@types/react-window": "^2.0.0",
    "@vitejs/plugin-react": "^5.1.3",
    "vite": "^7.3.1",
    "wrangler": "^3.107.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "autoprefixer": "^10.4.24",
    "hono": "^4.6.12",
    "postcss": "^8.5.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-intersection-observer": "^10.0.2",
    "react-router-dom": "^7.13.0",
    "react-window": "^2.2.6",
    "tailwindcss": "^4.1.18",
    "zustand": "^5.0.11"
  }
}
